{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/bc-ticker",
  "version": "0.1.0",
  "title": "Bc Ticker",
  "category": "widgets",
  "icon": "smiley",
  "description": "BC Ticker Gutenberg block for showing fixture data.",
  "supports": {
    "html": false
  },
  "attributes": {
    "ticker_title": {
      "type": "string"
    },
    "date_from": {
      "type": "string",
      "default": "#000000"
    }
  },
  "textdomain": "bc-ticker",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}